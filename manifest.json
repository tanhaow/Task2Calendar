{
    "manifest_version": 2,
    "name": "TaskToCalendar",
    "version": "1.0",
    "permissions": ["activeTab", "storage", "https://*/*"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html"
    }
  }
  